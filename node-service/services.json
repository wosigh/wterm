{
	"id": "us.ryanhope.wterm.tty.service",
	"description": "TTY Service",
	"commandTimeout": 5,
	"services": [
		{
			"name": "us.ryanhope.wterm.tty.service",
			"description": "TTY Service",
			"assistant": "TTYService",
			"commands": [
				{ 
					"name": "open", 
					"assistant": "TTYOpenAssistant",
					"subscribe": true,
					"public": true
				},
				{ 
					"name": "run", 
					"assistant": "TTYRunAssistant",
					"public": true
				},
				{ 
					"name": "kill", 
					"assistant": "KillAssistant",
					"public": true
				}
			]
		}
	]
}
